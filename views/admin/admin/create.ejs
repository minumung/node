<%- include('../layout/classicAdmin.ejs') %>
<%- include('../layout/headerAdmin.ejs') %>
<%- include('../common/libTable.ejs') %>
<% const key = 'admin'%>
<script>
	$(function(){
		$(".submit-btn").click(function(){
			var admin_id = $("input[name=<%=key%>_id]");
			var password = $("input[name=password]");
			var name = $("input[name=name]");
			var write_by = $("input[name=write_by]");
			
			var param = {
					"<%=key%>_id" : admin_id.val(),
					"password" : password.val(),
					"name" : name.val(),
					"write_by": write_by.val()
			}
			
			ajaxCallPost("/supervise/<%=key%>/create", param, function(res){
				if(res.success){
					alert("작업이 완료되었습니다.");
					location.href="/supervise/<%=key%>/list"
				}
			},function(){})
		})
	})
</script>

<!-- Static Table Start -->
<div class="all-content-wrapper"
	style="min-height: 900px; background: white;">



	<div id="myTabContent" class="tab-content custom-product-edit"
		style="padding: 0px 50px 0 50px">
		<div class="product-tab-list tab-pane fade active in" id="description">
			<div class="row">
				<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
					<div class="review-content-section">
					
					 <div class="sparkline13-hd">
                                <div class="main-sparkline13-hd">
                                    <h1>관리자 <span class="table-project-n">Data</span> Table</h1>
                                </div>
                            </div>
					
						<div id="dropzone1" class="pro-ad">
							<form class="dropzone dropzone-custom needsclick add-professors">
							<input type="hidden" name="write_by" value="<%=ADMIN.admin_seq %>">
								<div class="row">
									<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
										<div class="form-group">
											<input name="<%=key%>_id" type="text" class="form-control"
												placeholder="아이디">
										</div>
										<div class="form-group">
											<input name="password" type="password" class="form-control"
												placeholder="비밀번호">
